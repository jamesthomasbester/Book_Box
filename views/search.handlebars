                <img src="{{thumbnail}}" alt="">
                <h3>{{title}}</h3>
                <div class="price">{{price}}</div>
            </div>
        {{/each}}
        {{#each Category}}
            <div class="product" data-name="p-1">
                <img src="{{thumbnail}}" alt="">
                <h3>{{title}}</h3>
                <div class="price">{{price}}</div>
            </div>
        {{/each}}

